        <activity
            android:name="com.example.databytes.hospital_new"
            android:exported="false"
            android:label="Sign Up"
            android:parentActivityName="com.example.databytes.hospital_login" />
        <activity
            android:name="com.example.databytes.hospital_login"
            android:exported="false"
            android:label="Hospital Login"
            android:parentActivityName="com.example.databytes.MainActivity" />
        <activity
            android:name="com.example.databytes.patient_new"
            android:exported="false"
