            android:parentActivityName="com.example.databytes.patient_login" />
        <activity
            android:name="com.example.databytes.patient_login"
            android:exported="false"
            android:label="Patient&apos;s login"
            android:parentActivityName="com.example.databytes.MainActivity" />
        <activity
            android:name="com.example.databytes.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.databytes.CaptureAct"
            android:screenOrientation="fullSensor"
            android:stateNotNeeded="true"
            android:windowSoftInputMode="stateAlwaysHidden" />
    </application>

</manifest>