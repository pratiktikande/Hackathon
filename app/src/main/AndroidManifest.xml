        android:roundIcon="@drawable/lifylogonew"
        android:supportsRtl="true"
        android:theme="@style/Theme.Emergon"
        tools:targetApi="31">
        <activity
            android:name="com.example.databytes.crpres"
            android:exported="false"
            android:parentActivityName="com.example.databytes.hlogin"/>
        <activity
            android:name="com.example.databytes.Document_upload_page"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name="com.example.databytes.hos_scan_res"
            android:exported="false"
            android:parentActivityName="com.example.databytes.hlogin"/>
        <activity
            android:name="com.example.databytes.neurology"
            android:exported="false"
            android:label="Neurology"
            android:parentActivityName="com.example.databytes.casefolder" />
        <activity
            android:name="com.example.databytes.skin"
            android:exported="false"
            android:label="Dermatology"
            android:parentActivityName="com.example.databytes.casefolder" />
        <activity
            android:name="com.example.databytes.emergency"
            android:exported="false"
            android:label="Cardiology"
            android:parentActivityName="com.example.databytes.casefolder" />
        <activity
            android:name="com.example.databytes.medicine"
            android:exported="false"
            android:label="Medicine"
            android:parentActivityName="com.example.databytes.casefolder" />
        <activity
            android:name="com.example.databytes.gaurdian_list_view"
            android:exported="false"
            android:label="Guardian List"
            android:parentActivityName="com.example.databytes.ui.slideshow.SlideshowFragment" />
        <activity
            android:name="com.example.databytes.Scanner_page"
            android:exported="false" />
        <activity
            android:name="com.example.databytes.Addhar_verfi_page"
            android:exported="false" />
        <activity
            android:name="com.example.databytes.hlogin"
            android:exported="false" />
        <activity
